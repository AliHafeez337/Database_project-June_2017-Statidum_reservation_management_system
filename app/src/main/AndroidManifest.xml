            <intent-filter>
                <action android:name="ali.stadiumreservationmanagementsystem.Stadium" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateSchedule">
            <intent-filter>
                <action android:name="ali.stadiumreservationmanagementsystem.UpdateSchedul" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddCustomer">
            <intent-filter>
                <action android:name="ali.stadiumreservationmanagementsystem.AddCustomer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".removeCustomer"></activity>
    </application>

</manifest>