                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".createAccount">
            <intent-filter>
                <action android:name="ali.stadiumreservationmanagementsystem.createAccount" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Credit">
            <intent-filter>
                <action android:name="ali.stadiumreservationmanagementsystem.Credit" />

                <category android:name="android.intent.category.DEFAULT" />
